<div class="col-xs-9">
  <h2>Users</h2>
  <%= render 'partial/userinfo', book: @book, user: current_user %>

  <table>
    <thead>


    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
    <tr>


    </thead>
  </table>
    <% @users.each do |user| %>
      <th><%= attachment_image_tag user , :profile_image, size: "40x40" %></th>
      <th><%= user.name %></th>
      <th><%= link_to "Show", user_path(user.id) %></th>
    <% end %>
    </tr>
</div>